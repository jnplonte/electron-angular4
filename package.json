{
  "name": "otomeyt-desktop",
  "productName": "Otomeyt Desktop Application",
  "productTeam": "Otomeyt Team",
  "version": "1.0.0",
  "main": "./dist/main-site.js",
  "devDependencies": {
    "@angular/cli": "^1.4.5",
    "@angular/compiler-cli": "^5.0.0",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "child_process": "^1.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.0.1",
    "electron-prebuilt": "^1.4.13",
    "electron-winstaller": "^2.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.5",
    "shelljs": "^0.7.8",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.2.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngx-translate/core": "^8.0.0",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "fs": "0.0.1-security",
    "fs-extra": "^4.0.2",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "pouchdb": "^6.3.4",
    "pouchdb-authentication": "^0.5.5",
    "pouchdb-browser": "^6.3.4",
    "pouchdb-find": "^6.3.4",
    "rxjs": "^5.5.2",
    "typescript": "^2.6.1",
    "zone.js": "^0.8.12"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --inline --progress",
    "start:electron": "electron .",
    "test": "karma start ./config/karma.common.js",
    "lint": "tslint --type-check --project 'src/tsconfig.json' --config 'tslint.json'",
    "prebuild": "rimraf dist",
    "build": "webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "postbuild": "node ./config/electron/encode-config.js",
    "generate:component": "node ./config/generate.js \"component\"",
    "generate:directive": "node ./config/generate.js \"directive\"",
    "generate:pipe": "node ./config/generate.js \"pipe\"",
    "generate:service": "node ./config/generate.js \"service\"",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./config/electron/icons/otomeyt.icns --prune=true --out=./dist/release-builds",
    "package:win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./config/electron/icons/otomeyt.ico --prune=true --out=./dist/release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Otomeyt\"",
    "package:linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=./config/electron/icons/otomeyt.ico --prune=true --out=./dist/release-builds",
    "create:installer-win": "node ./dist/build-windows.js",
    "create:installer-mac": "electron-installer-dmg ./dist/release-builds/Otomeyt-darwin-x64/Otomeyt.app otomeyt-app --out=./dist/release-builds/installers/mac --overwrite --icon=./config/electron/icons/otomeyt.icns"
  },
  "keywords": [
    "nodejs",
    "electron",
    "angular",
    "typescript"
  ],
  "author": "John Paul Onte <jnpl.onte@gmail.com>",
  "license": "MIT"
}
